{
  "/auth/v1/login": { "POST": "anon" },
  "/auth/v1/register": { "POST": "anon" },
  "/auth*": { "GET": "*" },
  "/api/v1/users/count/getCount": { "GET": "*" },
  "/api/v1/users*": {
    "ALL": "admin",
    "PUT": "author,moderator",
    "GET": "moderator"
  },
  "/api/v1/forums*": { "GET": "*" },
  "/api/v1/subforums/moderators*": { "GET": "moderator, admin" },
  "/api/v1/subforums/new-moderator": { "POST": "admin" },
  "/api/v1/subforums/remove-moderator": { "POST": "admin" },
  "/api/v1/subforums/update-moderator": { "PUT": "admin" },
  "/api/v1/subforums": { "GET": "*" },
  "/api/v1/subjects/lock*": { "PUT": "moderator,admin" },
  "/api/v1/subjects*": { "POST": "author,moderator,admin", "GET": "*" },
  "/api/v1/posts*": {
    "ALL": "admin",
    "POST": "author,moderator",
    "GET": "*",
    "DELETE": "moderator"
  },
  "/api-docs*": { "GET": "*" },
  "/favicon.ico*": { "GET": "*" },
  "/css*": { "GET": "*" },
  "/js*": { "GET": "*" },
  "/": { "GET": "*" }
}
